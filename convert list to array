import array
sum=0
a=array.array("i",[1,2,3,4])
for i in a:
    sum=sum+i
print("sum:",sum)

              (or)
              
import array
sum=0
i=[5,6,7,8,9]
a=array.array("i",[])
a.fromlist(i)
for i in a:
    sum=sum+i
print("sum:",sum)

             (or)
             
import array
sum=0
i=[5,6,7,8,9]
a=array.array("i",[])
a.fromlist(i)
print(a)
