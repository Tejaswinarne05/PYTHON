print("circle")
import math
r=int(input("enetr the number:"))
print("area ",math.pi*r**2)
print("circumference ",4*math.pi*r)

print("rectangle")
l=int(input("enetr the number:"))
b=int(input("enetr the number:"))
print("area ",l*b)
print("perimeter ",2*(l+b))

print("sqare")
s=int(input("enetr the number:"))

print("area ",s**2)
print("perimeter ",4*s)
