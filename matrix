row=int(input("rows:"))
col=int(input("col:"))
a=[]
for i in range(row):
    b=[]
    for j in range(col):
        b.append(input("enter:"))
    a.append(b)
for i in range(row):
    for j in range(col):
        print(a[i][j],end=" ")
    print()

