a="good_morning"
print(a.capitalize())
print(a.upper())
print(a.lower())
print(a.title())
print(a.swapcase())
print(a.center(20,"!"))
print(a.count('good'))
print(a.replace("morning","night"))
b="*"
print(b.join(a))
print(a.isupper())
print(a.islower())
print(a.isalpha())
print(a.isalnum())
print(a.isdigit())
print(a.istitle())
print(a.startswith("g"))
print(a.endswith("g"))
print(a.find("good"))
print(len(a))
print(min(a))
print(max(a))
