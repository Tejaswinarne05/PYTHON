r=[3,4],[2,1]
c=[1,2],[3,4]
res=[0,0],[0,0]
n=len(r)
for i in range(n): 
    for j in range(n):
        res[i][j]=r[i][j]+c[i][j]
     

print(res)
print()


r=[3,4],[2,1]
c=[1,2],[3,4]
res=[0,0],[0,0]
n=len(r)
for i in range(n): 
    for j in range(n):
        res[i][j]=r[i][j]-c[i][j]
     

print(res)
print()
