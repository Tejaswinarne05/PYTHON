a=int(input("enter the number"))
b=int(input("enter the number"))
c=int(input("enter the number"))
if(a>b):
    if(a>c):
        print("greater number",a)
    else:
            print("greater value",c)
else:
     if(b>c):
         print("greater value",b)
     else:
             print("greater value",c)
